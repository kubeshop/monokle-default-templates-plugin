{
  "name": "Monokle Default Templates Plugin",
  "author": "kubeshop.io",
  "version": "1.0.0",
  "description": "This plugin provides the default resource templates for Monokle",
  "license": "MIT",
  "repository": "https://github.com/kubeshop/monokle-default-templates-plugin",
  "monoklePlugin": {
    "id": "io.kubeshop.monokle.plugins.default-templates",
    "helpUrl": "https://github.com/kubeshop/monokle-default-templates-plugin",
    "modules": [
      {
        "type": "template",
        "path": "deployment-template"
      },
      {
        "type": "template",
        "path": "basic-job-template"
      },
      {
        "type": "template",
        "path": "basic-pod-template"
      },
      {
        "type": "template",
        "path": "advanced-pod-template"
      },
      {
        "type": "template",
        "path": "basic-role-template"
      },
      {
        "type": "template",
        "path": "rolebinding-serviceaccount-template"
      },
      {
        "type": "template",
        "path": "role-rolebinding-serviceaccount-template"
      },
      {
        "type": "template",
        "path": "basic-service-template"
      }
    ]
  }
}
